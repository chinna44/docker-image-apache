{
    "family": mytaskdefinition,
    "containerDefinitions": [
        {
            "image": "REPOSITORY_URI:latest",
            "name": apache-task,
	    "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
             "FARGATE"
    ],
    "networkMode": "awsvpc"
}
